<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.StatusBar.SystemPanel" parent="@*android:style/TextAppearance.StatusBar">
        <item name="android:textAppearance">?android:textAppearance</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#fff</item>
    </style>

    <style name="TextAppearance" />

    <style name="TextAppearance.NotificationGuts" parent="@style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:fontFamily">roboto-regular</item>
    </style>

    <style name="TextAppearance.NotificationGuts.Secondary" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:textColor">#ddffffff</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Primary" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#fff</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Radio" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:textColor">#fff</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Button" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ddffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="edit_theme" parent="@android:style/Theme.DeviceDefault">
        <item name="android:colorBackground">#000</item>
    </style>
</resources>