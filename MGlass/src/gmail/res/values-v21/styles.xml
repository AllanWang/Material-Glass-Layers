    <style name="RecipientEditTextViewBase" parent="@android:style/Widget.DeviceDefault.AutoCompleteTextView">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">32.0dip</item>
        <item name="android:popupBackground">#bb000000</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
    </style>

    <style name="AbstractComposeArea">
        <item name="android:layout_gravity">center_horizontal</item>
