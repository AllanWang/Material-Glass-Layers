<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.pitchedapps.material.glass.layers.module">

    <!-- This will set the Icon, Target package to overlay & priority for this apk -->
    <application android:icon="@drawable/theme_application_icon" />
    <overlay android:targetPackage="@string/target_package" android:priority="99" />

    <!-- This tells the layers application about this theme apk -->
    <meta-data android:name="rom" android:value="Material_Glass" />
    <meta-data android:name="infoShort" android:value="@string/info_short" />
    <meta-data android:name="infoLong" android:value="@string/info_long" />
    <meta-data android:name="author" android:value="Pitched Apps" />
</manifest>