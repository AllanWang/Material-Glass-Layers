<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.Material.Notification.Emphasis"
        parent="@android:style/TextAppearance.Material.Notification">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14.0sp</item>
    </style>
    <style name="TextAppearance.Material.Notification"
        parent="@android:style/TextAppearance.Material">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#b3ffffff</item>

    </style>
    <style name="TextAppearance.Material.Notification.Title"
        parent="@android:style/TextAppearance.Material.Notification">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="TextAppearance.Material.Notification.Info" parent="@android:style/TextAppearance.Material">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#fff</item>
    </style>
    <style name="TextAppearance.Material.Notification.Line2" parent="@android:style/TextAppearance.Material">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearance.Material.Notification.Time" parent="@android:style/TextAppearance.Material">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Widget.Material.Notification.ProgressBar" parent="@android:style/Widget.Material.ProgressBar.Horizontal" />

    <!--Bar below notification with action buttons-->
    <color name="notification_action_list">#748B96</color>
    <color name="notification_action_list_dark">#222</color>
    <color name="config_defaultNotificationColor">#fff</color>
    <color name="notification_text_default_color">#eee</color>
    <color name="notification_material_background_color">#1b1b1b</color>
    <color name="notification_default_color">#eee</color>
    <color name="notification_icon_default_color">#fff</color>
    <color name="notification_progress_background_color">
        @*android:color/secondary_text_material_dark
    </color>

    <!--android m-->
