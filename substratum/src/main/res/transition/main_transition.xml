<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    android:transitionOrdering="together" >
    <changeBounds
        android:duration="1000"
        android:interpolator="@android:interpolator/fast_out_linear_in">
        <targets>
            <target android:targetId="@id/logo" />
        </targets>
    </changeBounds>
    <changeBounds
        android:duration="1000"
        android:startDelay="1200"
        android:interpolator="@android:interpolator/decelerate_quint">
        <targets>
            <target android:targetId="@id/text" />
        </targets>
    </changeBounds>
    <changeBounds
        android:duration="1000"
        android:startDelay="1300"
        android:interpolator="@android:interpolator/decelerate_quint">
        <targets>
            <target android:targetId="@id/text1" />
        </targets>
    </changeBounds>
    <changeBounds
        android:duration="1000"
        android:startDelay="1400"
        android:interpolator="@android:interpolator/decelerate_quint">
        <targets>
            <target android:targetId="@id/text2" />
        </targets>
    </changeBounds>
    <changeBounds
        android:duration="1000"
        android:startDelay="1500"
        android:interpolator="@android:interpolator/decelerate_quint">
        <targets>
            <target android:targetId="@id/text3" />
            <target android:targetId="@id/linear" />
        </targets>
    </changeBounds>
    <!--<changeBounds-->
    <!--android:interpolator="@android:interpolator/fast_out_linear_in"-->
    <!--android:startDelay="200" />-->
</transitionSet>